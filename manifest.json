{
  "name": "Sbid Populator",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "src/modules/popup.html",
    "default_title": "Sbid Populator"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "permissions": ["activeTab", "storage"]
}
